DEV="~/Developer/"
TMP="${DEV}/tmp"
WORK="${DEV}/work"
PERSONAL="${DEV}/personal"
ANIMO="${WORK}/animo"
HYPERLEDGER="${WORK}/hyperledger"

# Directories
alias tmp="cd ${TMP}"
alias work="cd ${WORK}"
alias per="cd ${PERSONAL}"
alias animo="cd ${ANIMO}"
alias hype="cd ${HYPERLEDGER}"

# Editting
alias v="vim"
alias nv="nvim"
alias c="code ."
alias als="nvim ~/.config/zsh/alias.zsh"

# Sourcing
alias szsh="source ~/.zshrc"
